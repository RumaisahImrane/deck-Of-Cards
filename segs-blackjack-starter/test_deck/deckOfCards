// Creating a deck of 52 cards

class deckOfCards {
    //Creating an array to hold cards
    myCards = []; // Empty array
    const values = ['2', '3', '4', '5', '6', '7', '8', '9', '10', 'Jack', 'Queen', 'King', 'Ace'];

    =
    for (let i = 0; i < 4; i++) {  // 4x decks
        for (let value of values) {
        myCards.push(value);
  }
}
